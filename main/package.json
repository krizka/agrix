{
  "name": "kickstart-mantrajs-webpack",
  "version": "1.0.0",
  "description": "Kickstart a project with Meteor, React and react-router fast!",
  "main": "src/server.js",
  "browser": "src/client.js",
  "scripts": {
    "lint": "eslint ./src/lib ./src/client ./src/server --ext .js --ext .jsx",
    "lintfix": "npm run lint -- --fix",
    "testonly": "mocha .scripts/mocha_boot.js src/client/**/tests/**/*.js --compilers js:babel-core/register",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js,jsx"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "domready": "^1.0.8",
    "mantra-core": "^1.5.0",
    "react": "^15.0.1",
    "react-addons-create-fragment": "^15.0.1",
    "react-addons-css-transition-group": "^15.0.1",
    "react-addons-linked-state-mixin": "^15.0.1",
    "react-addons-perf": "^15.0.1",
    "react-addons-pure-render-mixin": "^15.0.1",
    "react-addons-transition-group": "^15.0.1",
    "react-addons-update": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-mounter": "^1.2.0",
    "react-ui-tree": "git+https://github.com/krizka/react-ui-tree.git"
  },
  "devDependencies": {
    "babel-core": "6.x.x",
    "babel-plugin-react-require": "2.x.x",
    "babel-polyfill": "6.x.x",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-root-slash-import": "1.x.x",
    "chai": "3.x.x",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "2.7.x",
    "eslint-plugin-react": "4.3.x",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "jsdom": "^8.3.1",
    "jsx-control-statements": "^3.1.1",
    "mocha": "2.x.x",
    "react-addons-test-utils": "^15.0.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.3",
    "sinon": "1.17.x",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.15",
    "webpack-hot-middleware": "^2.4.1",
    "expose-loader": "^0.7.1",
    "babel": "^6.5.2"
  }
